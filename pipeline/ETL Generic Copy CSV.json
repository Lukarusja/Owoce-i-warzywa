			"DataWarehouseName": {
				"type": "string",
				"defaultValue": "Edgewell"
			},
			"Filename": {
				"type": "string",
				"defaultValue": "tactical-edgewell"
			},
			"Slot": {
				"type": "string",
				"defaultValue": "dev"
			}
		},
		"variables": {
